@{
    ViewBag.Title = "体质检测记录表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CustomerModule/Physique/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>姓名：</th>
            <td class='formValue'>
                <label id="name" />
            </td>
            <th class='formTitle'>电话：</th>
            <td class='formValue'>
                <label id="tel" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>体质监测结果：</th>
            <td class='formValue'>
                <label id="result" />
            </td>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="createtime" />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>
