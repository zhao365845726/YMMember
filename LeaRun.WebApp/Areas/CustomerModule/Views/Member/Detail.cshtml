@{
    ViewBag.Title = "会员明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CustomerModule/Member/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebLable(data);
                if (data.Sex == "1") {
                    $("#Sex").text("男");
                } else if (data.Sex == "0") {
                    $("#Sex").text("女");
                }
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <div class="bd" style="border-bottom: none; margin: 1px;">
        <div class="tipstitle_title settingtable bold bd todayInfoPanelTab rightPanelTitle_normal">
            <div class="tab_list_top" style="position: absolute">
                <div id="Tabbasic" class="tab_list bd actived" onclick="Tabchange('basic')">基本信息</div>
                @*<div id="Tabdiagnosis" class="tab_list bd " onclick="Tabchange('diagnosis')">诊断记录</div>*@
            </div>
        </div>
    </div>
    <div class="ScrollBar" style="margin: 1px">
        <!--基本信息-->
        <div id="basic" class="tabPanel">
            <table class="form">
                <tr>
                    <th class='formTitle'>姓名：</th>
                    <td class='formValue'>
                        <label id="Name" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>年龄：</th>
                    <td class='formValue'>
                        <label id="Age" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>性别：</th>
                    <td class='formValue'>
                        <label id="Sex" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>电话：</th>
                    <td class='formValue'>
                        <label id="Tel" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>生日：</th>
                    <td class='formValue'>
                        <label id="Birthday" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>职业：</th>
                    <td class='formValue'>
                        <label id="Profession" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>家庭住址：</th>
                    <td class='formValue'>
                        <label id="HomeAddress" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>病种：</th>
                    <td class='formValue'>
                        <label id="Disease" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>亚健康：</th>
                    <td class='formValue'>
                        <label id="SubHealth" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>组别：</th>
                    <td class='formValue'>
                        <label id="SafeTangGroup" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>组别号：</th>
                    <td class='formValue'>
                        <label id="GroupNumber" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>入会时间：</th>
                    <td class='formValue'>
                        <label id="MembershipDate" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>到期时间：</th>
                    <td class='formValue'>
                        <label id="ExpireDate" />
                    </td>
                </tr>
                <tr></tr>
            </table>
        </div>
    
        <div id="diagnosis" class="tabPanel" style="display:none;">
            <table class="form">
                <tr>
                    <th class='formTitle'>中医诊断：</th>
                    <td class='formValue'>
                        <label id="ChineseDiagnosis"/>
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>西医诊断：</th>
                    <td class='formValue'>
                        <label id="WESTDiagnosis"/>
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>病历情况：</th>
                    <td class='formValue'>
                        <label id="MedicalRecord"/>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</form>
