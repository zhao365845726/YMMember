@{
    ViewBag.Title = "附件》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CustomerModule/Enclosure/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>附件类型：</th>
            <td class='formValue'>
                <label id="Type" />
            </td>
            <th class='formTitle'>文件名称：</th>
            <td class='formValue'>
                <label id="FileName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>文件路径：</th>
            <td class='formValue'>
                <label id="FilePath" />
            </td>
            <th class='formTitle'>文件大小：</th>
            <td class='formValue'>
                <label id="FileSize" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>文件类型：</th>
            <td class='formValue'>
                <label id="FileType" />
            </td>
            <th class='formTitle'>创建日期：</th>
            <td class='formValue'>
                <label id="CreateDate" />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>
