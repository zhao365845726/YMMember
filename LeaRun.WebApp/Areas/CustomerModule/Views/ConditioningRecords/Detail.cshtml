@{
    ViewBag.Title = "调理记录》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CustomerModule/ConditioningRecords/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebLable(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <div class="bd" style="border-bottom: none; margin: 1px;">
        <div class="tipstitle_title settingtable bold bd todayInfoPanelTab rightPanelTitle_normal">
            <div class="tab_list_top" style="position: absolute">
                <div id="Tabbasic" class="tab_list bd actived" onclick="Tabchange('basic')">调理信息</div>
            </div>
        </div>
    </div>
    <div class="ScrollBar" style="margin: 1px; overflow: hidden;">
        <!--基本信息-->
        <div id="basic" class="tabPanel">
            <table class="form">
                <tr style="display:none;">
                    <th class='formTitle'>会员ID：</th>
                    <td class='formValue'>
                        <label id="MemberId" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>处方类型：</th>
                    <td class='formValue'>
                        <label id="PrescriptionType" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>处方内容：</th>
                    <td class='formValue'>
                        <label id="PrescriptionContent" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>调理师类型：</th>
                    <td class='formValue'>
                        <label id="CulinarianType" />
                    </td>
                </tr>
                <tr>
                    <th class='formTitle'>调理结果：</th>
                    <td class='formValue'>
                        <label id="ConditioningResult" />
                    </td>
                </tr>
                <tr style="display:none;">
                    <th class='formTitle'>排序：</th>
                    <td class='formValue'>
                        <label id="Sort" />
                    </td>
                </tr>
                <tr></tr>
            </table>
        </div>
    </div>
</form>
